      <extension base="gml:AbstractFeatureType">
        <sequence>
          <element minOccurs="0" name="vedlikeholdsansvarlig" type="string">
            <annotation>
              <documentation>institusjon eller andre som har ansvar for vedlikehold av rute</documentation>
            </annotation>
          </element>
          <element name="friluftEierforhold" type="sosi:FriluftEierforholdType">
