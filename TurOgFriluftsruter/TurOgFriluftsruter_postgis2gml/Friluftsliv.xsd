          <element minOccurs="0" name="belysning" type="sosi:BelysningType">
            <annotation>
              <documentation>om det er permanent belysning langs ruten</documentation>
            </annotation>
          </element>
          <element name="merking" type="sosi:RuteMerkingType">
            <annotation>
              <documentation>forteller om det er merking  langs en sti, løype, veg, sykkelvei mv</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="skilting" type="boolean">
            <annotation>
              <documentation>om ruten er skiltet eller ikke (må skilles fra merking - skiltes ofte i viktige kryss og start- sluttpunkt)</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="ruteFølger" type="sosi:RuteFølgerType">
            <annotation>
              <documentation>hoveddelen av type trase som rute eller løype følger</documentation>
            </annotation>
          </element>
